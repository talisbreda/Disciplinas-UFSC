.data

	prompt: .asciiz "Insira uma temperatura em Fahrenheit: "
	result: .asciiz "A temperatura em Celsius é: "
	thirtytwo: .double 32
	nine: .double 9
	five: .double 5

.text
main:

	# $f0 -> temperature em F
	
	# Imprime prompt
	li	$v0, 4
	la	$a0, prompt
	syscall
	
	# Le temperatura do console como double
	li	$v0, 7
	syscall
	
	mov.d	$f12, $f0	# Argumento 1 -> valor em F
	jal	converte
	mov.d	$f12, $f0	# Move o retorno da funcao para $f12, que sera mostrado no console
	
	# Imprime string para resultado
	li	$v0, 4
	la	$a0, result
	syscall
	
	# Mostra o resultado no console
	li	$v0, 3
	syscall
	
	# Fecha o programa
	li	$v0, 10
	syscall
	
converte:

	# $f12 -> temperatura em F
	
	l.d	$f4, thirtytwo
	l.d	$f6, nine
	l.d	$f8, five
	
	sub.d	$f12, $f12, $f4
	div.d	$f12, $f12, $f6
	mul.d	$f0, $f12, $f8
	
	jr	$ra